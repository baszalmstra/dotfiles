[user]
	email = zalmstra.bas@gmail.com
	name = Bas Zalmstra
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
    pager = delta --plus-color="#012800" --minus-color="#340001"
	autocrlf = input

[interactive]
    diffFilter = delta --color-only
    
[url "ssh://git@git.smart-robotics.nl:"]
	insteadOf = https://git.smart-robotics.nl
